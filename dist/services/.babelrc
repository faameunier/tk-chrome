{
   "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "Chrome >= 59"
          ]
        }
      }
    ],
    ['minify', {
      builtIns: false,
      evaluate: true,
      mangle: true
    }],
  ],
  "plugins":[
    ["@babel/plugin-proposal-decorators", {"legacy": true}], 
    "@babel/plugin-proposal-class-properties"
  ],
  "sourceType": "script",
  "comments": false
}