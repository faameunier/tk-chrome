{
  "name": "Tab Killer",
  "version": "0.0",
  "description": "The tab serial killer",
  "permissions": ["http://*/*", "https://*/*", "tabs", "storage"],
  "options_page": "./app/options.html",
  "background": {
    "scripts": [
      "./scripts/lodash.js",
      "./lib/env.js",
      "./lib/utils.js",
      "./lib/queue.js",
      "./lib/memory.js",
      "./lib/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "./app/popup.html",
    "default_icon": {
      "16": "./images/get_started16.png",
      "32": "./images/get_started32.png",
      "48": "./images/get_started48.png",
      "128": "./images/get_started128.png"
    }
  },
  "icons": {
    "16": "./images/get_started16.png",
    "32": "./images/get_started32.png",
    "48": "./images/get_started48.png",
    "128": "./images/get_started128.png"
  },
  "manifest_version": 2
}
