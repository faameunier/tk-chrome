{
  "name": "Tabby",
  "version": "0.0.1",
  "version_name": "Beta",
  "description": "Make Chrome focus-friendly",
  "permissions": ["http://*/*", "https://*/*", "tabs", "storage"],
  "background": {
    "scripts": [
      "./lib/lodash.js",
      "./lib/psl.min.js",
      "./config/env.js",
      "./config/settingsProfiles.js",
      "./services/utils.js",
      "./services/queue.js",
      "./services/LRU.js",
      "./services/settings.js",
      "./services/memory.js",
      "./services/scorer.js",
      "./services/policy.js",
      "./services/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "./views/popup.html",
    "default_icon": {
      "16": "./assets/static/icons/tabby_icon_16.png",
      "32": "./assets/static/icons/tabby_icon_32.png",
      "48": "./assets/static/icons/tabby_icon_48.png",
      "128": "./assets/static/icons/tabby_icon_128.png"
    }
  },
  "icons": {
    "16": "./assets/static/icons/tabby_icon_16.png",
    "32": "./assets/static/icons/tabby_icon_32.png",
    "48": "./assets/static/icons/tabby_icon_48.png",
    "128": "./assets/static/icons/tabby_icon_128.png"
  },
  "manifest_version": 2
}
