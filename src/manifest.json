{
  "name": "Tab Killer",
  "version": "0.0",
  "description": "The tab serial killer",
  "permissions": ["http://*/*", "https://*/*", "tabs", "storage"],
  "background": {
    "scripts": [
      "./lib/lodash.js",
      "./lib/psl.min.js",
      "./config/env.js",
      "./services/utils.js",
      "./services/queue.js",
      "./services/LRU.js",
      "./services/memory.js",
      "./services/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "./views/popup.html",
    "default_icon": {
      "16": "./assets/static/icons/get_started16.png",
      "32": "./assets/static/icons/get_started32.png",
      "48": "./assets/static/icons/get_started48.png",
      "128": "./assets/static/icons/get_started128.png"
    }
  },
  "icons": {
    "16": "./assets/static/icons/get_started16.png",
    "32": "./assets/static/icons/get_started32.png",
    "48": "./assets/static/icons/get_started48.png",
    "128": "./assets/static/icons/get_started128.png"
  },
  "manifest_version": 2
}
