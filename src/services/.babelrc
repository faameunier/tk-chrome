{
   "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "Chrome >= 59"
          ]
        }
      }
    ],
  ],
  "plugins":[
    ["@babel/plugin-proposal-decorators", {"legacy": true}], 
    "@babel/plugin-proposal-class-properties"
  ],
  "sourceType": "script",
  "env": {
    "prod": {
      "presets": [['minify', {
        builtIns: false,
        evaluate: true,
        mangle: true
      }]],
      "comments": false
    },
  }
}